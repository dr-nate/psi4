set(LIBEFP_CONFIG_PATH ${CMAKE_CURRENT_LIST_DIR})
find_library(LIBEFP_LIBRARY NAMES efp PATHS ${LIBEFP_CONFIG_PATH}/lib 
             NO_DEFAULT_PATH)
find_path(LIBEFP_INCLUDE_DIR NAMES efp/efp.h PATHS ${LIBEFP_CONFIG_PATH}/include
             NO_DEFAULT_PATH)

add_library(efp INTERFACE)
target_include_directories(efp INTERFACE ${LIBEFP_INCLUDE_DIR})
target_link_libraries(efp INTERFACE ${LIBEFP_LIBRARY})

set(LIBEFP_INCLUDE_DIRS ${LIBEFP_INCLUDE_DIR})
set(LIBEFP_LIBRARIES ${LIBEFP_LIBRARY})
set(LIBEFP_FOUND TRUE)



